{% extends 'base.njk' %}

{% block body %}
  
  {{ content | safe }}

  <table class="table table-striped">
    <thead>
      <tr><th>#</th><th>Predictor</th><th>Score</th></tr>
    <thead>
    <tbody>
      {% for p in predictors %}
        {% if p.currentScore != 0 %}
          <tr><td>{{ loop.index }}</td><td>{{ p.name }}</td><td>{{ p.currentScore }}</td></tr>
        {% endif %}
      {% endfor %}
    </tbody>
  </table>
  
{% endblock %}